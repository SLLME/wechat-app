/* pages/home/detail/detail.wxss */
.app-container{
  width: 100%;
  height: 100%;
  font-size: 12px;
}
.public-detail-body-title {
  font-size: 1.2em;
  text-align: center;
  height: 50px;
  line-height: 50px;
  background: url("data:image/png;base64,");
  background-position: center;
  background-repeat: no-repeat;
  background-size: 18%;
}
.public-detail-content{
  background: #cccccc36;
  /* width: 100%; */
  padding: 5px;
  font-size: 1rem;
  overflow: auto;
  border-radius: 2%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  height: 100%;
}
.public-detail-body{
  flex: 1;
  overflow: auto;
}
.public-detail-body-content{
  border-radius: 2%;
  background: white;
  padding: 10px;
}
.public-detail-body-content label{
  color: #cccccc;
  /* width: 100px; */
  display: inline-block;
  /* text-align: right; */
  font-size: 0.8rem;
}
.public-detail-body-content span{
  display: inline-block;
  width: calc(100% - 100px);
  font-size: 0.8rem;
}
.public-detail-bottom{
  margin: 16px;
  display: flex;
  justify-content: space-around;
  margin-bottom: calc(16px + constant(safe-area-inset-bottom)); /* ios < 11.2*/
  margin-bottom: calc(16px + env(safe-area-inset-bottom)); /* ios >= 11.2*/
}


.weui-cell {
  padding: 8px 16px;
}

.my-divider {
  padding: 0px !important;
  border-top: 1px dashed #cccccc;
  margin: 5px auto;
}

/** 更改折叠面板样式 */
.public-detail-body .van-cell {
  font-size: 1rem;
  align-items: center;
  padding: 0 !important;
}

.public-detail-body .van-cell::after {
  border-bottom: 1px solid #ebedf000 !important;
}

.public-detail-body .van-collapse-item__content {
  padding: 12px 0 !important;
}

.public-detail-body .van-collapse-item {
  padding: 5px 0;
}

.public-detail-body .van-collapse-item--border::after {
  border-top: 1px solid #ebedf000 !important;
}

.public-detail-body .van-hairline--top-bottom::after {
  border: 0 solid #ebedf000 !important;
}

.public-detail-body .van-button {
  height: 36px !important;
  padding: 0 30px;
}

.van-cell-after::before {
  position: absolute;
  box-sizing: border-box;
  content: ' ';
  pointer-events: none;
  right: 16px;
  bottom: 0;
  left: 16px;
  border-bottom: 1px solid #ebedf0;
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
}

.van-hairline--top:after {
  border-top-width: 0px !important;
}

/** 最下边圆圈 */
.public-circular-content {
  display: flex;
  justify-content: space-around;
}

.public-circular {
  width: 20px;
  height: 20px;
  background: #cccccc38;
  border-radius: 50%;
}

table>tr>td {
  text-align: center;
 height:40px;
 display:inline-block;
 align-items:center;
 font-size:1rem;
}
.tr{
  display: flex;
  justify-content: space-around;
}

/** 编辑页面css */
.public-popup-content{
  width: 100%;
  height: 100%;
  padding: 5px 10px;
  box-sizing: border-box;
  background-color: rgb(204 204 204 / 50%);
}
.public-manual-content-form{
  width: 100%;
  border-radius: 3%;
  background-color: white;
  padding-bottom: 20px;
}

.my-weui-uploader__input-box{
  overflow: hidden;
  float: left;
  position: relative;
  margin-right: 8px;
  margin-bottom: 8px;
  width: 96px;
  height: 96px;
  box-sizing: border-box;
  background-color: #ededed;
}
.my-weui-uploader__input-box image{
  width: 100%;
}
.public-manual-content-tips{
  margin-top: 30px;
  color: #cccccc;
}
.public-manual-content-tips view:first-child{
  color: black;
}
.weui-cells:after, .weui-cells:before {
  content: " ";
  position: absolute;
  left: 0;
  right: 0;
  height: 1px;
  color: white;
  z-index: -2;
}

.weui-btn-area {
  margin: 20px 16px;
}
/** 页码 */
.page-number{
  text-align: center;
  margin-top: 10px;
}
