/* pages/home/manual/manual.wxss */
.public-manual-content{
  width: 100%;
  height: 100%;
  padding: 5px 10px;
  box-sizing: border-box;
  background-color: rgb(204 204 204 / 50%);
  font-size: 12px;
}
.public-manual-content-form{
  width: 100%;
  border-radius: 3%;
  background-color: white;
  padding-bottom: 20px;
  padding-top: 10px;
}

.my-weui-uploader__input-box{
  overflow: hidden;
  float: left;
  position: relative;
  margin-right: 8px;
  margin-bottom: 8px;
  width: 96px;
  height: 96px;
  box-sizing: border-box;
  background-color: #ededed;
}
.my-weui-uploader__input-box image{
  width: 100%;
}
.public-manual-content-tips{
  margin-top: 30px;
  color: #cccccc;
}
.public-manual-content-tips view:first-child{
  color: black;
}
.weui-cells:after, .weui-cells:before {
  content: " ";
  position: absolute;
  left: 0;
  right: 0;
  height: 1px;
  color: white;
  z-index: -2;
}

.weui-btn-area {
  margin: 20px 16px;
}
.weui-cell{
  padding: 8px 16px!important;
  font-size: 14px;
}
